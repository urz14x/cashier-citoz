    - database/factories/ProductFactory.php
    - database/factories/OrderFactory.php
    - database/factories/OrderDetailFactory.php
    - database/factories/StockAdjustmentFactory.php
    - database/migrations/2025_06_16_105343_create_categories_table.php
    - database/migrations/2025_06_16_105344_create_members_table.php
    - database/migrations/2025_06_16_105345_create_packages_table.php
